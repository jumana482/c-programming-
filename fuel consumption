#include <stdio.h>
int main()
{
    float liters, km;
    float mpg;
    scanf("%f", &liters);
    scanf("%f", &km);
    if (liters < 0 || km < 0)
    {
        printf("Invalid Input\n");
    } else 
    {
        float l_per_100km = (liters / km) * 100;
        float miles = km * 0.6214;
        float gallons = liters * 0.2642;
        mpg = miles / gallons;
        printf("Liters/100KM\n%.2f\n", l_per_100km);
        printf("Miles/gallons\n%.2f\n", mpg);
    }
    return 0;
}
