#include <stdio.h>

int main() {
    int a, b,c;
    scanf("%d %d %d", &a, &b, &c);
    if (a > b && a > c) {
        printf("a is largest then b and c\n");
    } else if (b > a && b > c) {
        printf("b is largest then a and c\n");
    } else if (c > a && c > b) {
        printf("c is largest then a and b\n");
    } else {
        printf("Two or more numbers are equal and largest\n");
    }

    return 0;
}
